>> +enable) {
>> +	struct cxl_memdev_ps_params params =3D {
>> +		.enable =3D enable,
>> +	};
>> +
>> +	return cxl_mem_ps_set_attrs(dev->parent, &params,
>> +cxl_ps_param_enable); }
>> +
